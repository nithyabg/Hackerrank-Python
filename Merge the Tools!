def merge_the_tools(string, k):
    n=len(string)
    start=0
    end=k
    while(end<=n):
        st=string[start:end]
        stt="".join(dict.fromkeys(st)) 
        print(stt)
        start=start+k
        end=end+k

if __name__ == '__main__':
    string, k = input(), int(input())
    merge_the_tools(string, k)
